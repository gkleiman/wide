- title 'wIDE | User Registration', true

.simple_form_container.ui-widget-content.ui-corner-all
  %h1.ui-widget-header Sign up
  = simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
    = f.input :user_name
    = f.input :email
    = f.input :password
    = f.input :password_confirmation
    = f.button :submit
= render :partial => "devise/shared/links"
